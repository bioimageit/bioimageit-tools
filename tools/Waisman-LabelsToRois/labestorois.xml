<tool id="LabelsToRois" name="LabelsToRois" version="1.0.0" python_template_version="3.5">
    <requirements>
        <package type="conda" env="LabelsToRois">python</package>
    </requirements>
    <command detect_errors="exit_code"><![CDATA[
        '$__fiji__' --console '$__tool_directory__Labels_To_Rois.py' --original ${original} --label ${label}
    ]]></command>
    <inputs>
        <param type="data" name="original" format="imagetiff" label="Original Image" help="Original image" />  
        <param type="data" name="label" format="imagetiff" label="Label Image" help="Label image" />  
    </inputs>
    <outputs>
<!--         <data name="csv" format="numbercsv" label="csv" /> 
 -->    </outputs>>
    <help><![CDATA[
        https://github.com/MouseLand/cellpose
    ]]></help>
    <citations>
    </citations>
</tool>
